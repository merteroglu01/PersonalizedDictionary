<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<TabPane prefHeight="400.0" prefWidth="640.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/10.0.1"
         xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.merteroglu.controller.Controller">
    <tabs>
        <Tab style="height: 640px;" text="Main">
            <content>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0">
                            <children>
                                <Label fx:id="infoText" alignment="CENTER" layoutX="184.0" layoutY="285.0" style="&#10;"
                                       text="Welcome to Your Personal Dictionary ! :)" textAlignment="CENTER"
                                       textFill="red" wrapText="false">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Label>
                                <TextField fx:id="addForeignWordTextField" layoutX="79.0" layoutY="233.0"
                                           promptText="Foreign Word"/>
                                <TextField fx:id="addNativeWordTextField" layoutX="272.0" layoutY="233.0"
                                           promptText="Native Word"/>
                                <Button fx:id="addWordBtn" layoutX="460.0" layoutY="233.0" mnemonicParsing="false"
                                        onAction="#saveToDictionary" text="Add" textAlignment="CENTER"/>
                                <Button fx:id="fullExerciseBtn" layoutX="460.0" layoutY="22.0" mnemonicParsing="false"
                                        onAction="#allTimeMod" text="Full Exercise"/>
                                <Button fx:id="checkBtn" layoutX="264.0" layoutY="84.0" mnemonicParsing="false"
                                        onAction="#check" text="Check !" textAlignment="CENTER"/>
                                <TextField fx:id="tryForeignWordTextField" focusTraversable="false" layoutX="87.0"
                                           layoutY="136.0" promptText="Foreign Word"/>
                                <TextField fx:id="tryNativeWordTextField" layoutX="79.0" layoutY="144.0"
                                           promptText="Native Word"/>
                                <Text fx:id="foreignWordText" layoutX="78.0" layoutY="102.0" lineSpacing="3.0"
                                      strokeType="OUTSIDE" strokeWidth="0.0" text="ForeignWordText"
                                      textAlignment="CENTER"/>
                                <Text fx:id="nativeWordText" layoutX="272.0" layoutY="153.0" lineSpacing="3.0"
                                      strokeType="OUTSIDE" strokeWidth="0.0" text="NativeWordText"
                                      textAlignment="CENTER"/>
                                <ToggleButton fx:id="changeInputAreaToggleBtn" layoutX="148.0" layoutY="328.0"
                                              mnemonicParsing="false" onAction="#changeExerciseType"
                                              text="Change Exercise Type"/>
                                <Button layoutX="264.0" layoutY="144.0" mnemonicParsing="false" text="Next Word"/>
                                <Button layoutX="414.0" layoutY="328.0" mnemonicParsing="false" text="Shuffle Mod"/>
                                <Button layoutX="383.0" layoutY="111.0" mnemonicParsing="false" text="Show the Answer"/>
                                <Button fx:id="weeklyExerciseBtn" layoutX="174.0" layoutY="45.0" mnemonicParsing="false"
                                        onAction="#weeklyMod" text="Weekly Exercise"/>
                                <Button fx:id="dailyExerciseBtn" layoutX="69.0" layoutY="16.0" mnemonicParsing="false"
                                        onAction="#dailyMod" text="Daily Exercise"/>
                            </children>
                        </AnchorPane>
                    </children>
                </VBox>
            </content>
        </Tab>
        <Tab text="Untitled Tab 2">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
            </content>
        </Tab>
        <Tab text="Untitled Tab">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
            </content>
        </Tab>
    </tabs>
</TabPane>